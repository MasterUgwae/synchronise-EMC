@article{math11102325,
    AUTHOR = {Klinshov, Vladimir V. and Zlobin, Alexander A.},
    TITLE = {Kuramoto Model with Delay: The Role of the Frequency Distribution},
    JOURNAL = {Mathematics},
    VOLUME = {11},
    YEAR = {2023},
    NUMBER = {10},
    ARTICLE-NUMBER = {2325},
    URL = {https://www.mdpi.com/2227-7390/11/10/2325},
    ISSN = {2227-7390},
    ABSTRACT = {The Kuramoto model is a classical model used for the describing
                of synchronization in populations of oscillatory units. In the
                present paper we study the Kuramoto model with delay with a focus
                on the distribution of the oscillatorsâ€™ frequencies. We consider
                a series of rational distributions which allow us to reduce the
                population dynamics to a set of several delay differential
                equations. We use the bifurcation analysis of these equations to
                study the transition from the asynchronous to synchronous state.
                We demonstrate that the form of the frequency distribution may
                play a substantial role in synchronization. In particular, for
                Lorentzian distribution the delay prevents synchronization, while
                for other distributions the delay can facilitate synchronization.
                },
    DOI = {10.3390/math11102325},
}



